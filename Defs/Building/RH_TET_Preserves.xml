<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>RH_TET_PicklingCrock</defName>
		<label>Pickling Crock</label>
		<description>A stone crock for pickling meats and vegetables.</description>
		<thingClass>TheEndTimes.Building_PicklingCrock</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/RH_TET_PicklingCrock</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
			<shadowData>
				<volume>(0.75, 0.35, 0.33)</volume>
			</shadowData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<uiIconPath>UI/Icons/Food/RH_TET_PicklingCrock_MenuIcon</uiIconPath>
		<fillPercent>0.45</fillPercent>
		<constructionSkillPrerequisite>3</constructionSkillPrerequisite>
		<pathCost>60</pathCost>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>0.1</Flammability>
		</statBases>
		<costList>
			<WoodLog>10</WoodLog>
		</costList>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Stony</li>
		</stuffCategories>
		<comps>
			<li Class="CompProperties_TemperatureRuinable">
				<minSafeTemperature>-1</minSafeTemperature>
				<maxSafeTemperature>32</maxSafeTemperature>
				<progressPerDegreePerTick>0.00001</progressPerDegreePerTick>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>false</rotatable>
		<designationCategory>Production</designationCategory>
		<researchPrerequisites>
			<li>RH_TET_FoodPreservation</li>
		</researchPrerequisites>
	</ThingDef>

